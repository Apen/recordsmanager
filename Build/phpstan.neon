parameters:
  level: 5
  reportUnmatchedIgnoredErrors: false
  bootstrapFiles:
    - ../Resources/Private/Php/PHP_XLSXWriter/xlsxwriter.class.php
  excludePaths:
    - ../Build/*
    - **/Resources/*
  ignoreErrors:
    - "#Undefined variable: \\$_EXTKEY#"
    - "#Constant .*? not found.#"
    - "#Comparison operation \">\" between int<1, max> and 0 is always true.#"